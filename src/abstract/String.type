/**
 * minimal complete string operations:
 * concatenate, compare, copy, length, substring
 */

type String {

    String concatenate(String self, String another);

    int compare(String self, String another);

    String copy(String self);

    int indexOf(String self, String p, int startIndex);

    int size(String self);

    String substring(String self, int startIndex, int endIndex);

    int codePointAt(String self, int index);
};
